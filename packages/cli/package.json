{
  "author": "Sequelize Team",
  "bin": {
    "sequelize": "./bin/run.js"
  },
  "bugs": "https://github.com/sequelize/sequelize/issues",
  "dependencies": {
    "@oclif/core": "^3.19.2",
    "@oclif/plugin-help": "^6.0.13",
    "@oclif/plugin-plugins": "^4.2.3",
    "@types/inquirer": "^9.0.7",
    "chalk": "^5.3.0",
    "cosmiconfig": "^9.0.0",
    "inquirer": "^9.2.14",
    "zod": "^3.22.4"
  },
  "description": "Sequelize CLI",
  "devDependencies": {
    "@oclif/test": "3.1.17",
    "oclif": "4.4.15"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": "./lib/index.js",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://sequelize.org/docs/v7/cli/",
  "keywords": [],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@sequelize/cli",
  "oclif": {
    "bin": "sequelize",
    "dirname": "sequelize",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "migration": {
        "description": "Commands for managing database migrations"
      }
    }
  },
  "repository": "https://github.com/sequelize/sequelize",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "0.0.0-development"
}
